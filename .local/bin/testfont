#!/bin/bash
#
# Test a font for all the testies

clear

xrdb -query | grep URxvt.font | sed 's/^[^-]*://g' | sed 's/\t//g'

echo ''

echo 'Hello World!'

echo 'Iil1 | 0Oo'

echo 'ABCDEFGHIJKLMNOPQRSTUVWXYZ'

echo 'abcdefghijklmnopqrstuvwxyz'

echo '1234567890 "`~!@#$%^&*_-=+"'

echo '" ` ~ ! @ # $ % ^ & * _ - = + "'

echo ', . () <> [] {} \ | ? / : ;'

echo ''

echo 'The quick brown fox jumps over the lazy dog.'

echo ''

echo 'Lorem ipsum dolor sit amet, consectetur adipiscing elit. In lacinia dui,'
echo 'dapibus eros vel, posuere turpis. Nulla cursus neque sed dignissim.'
echo 'Vestibulum sit amet dolor turpis. Integer bibendum vulputate convallis.'
echo 'Nulla lacus, aliquam eget libero congue, commodo dignissim neque. Etiam quis'
echo 'erat. Nullam sagittis tempor augue, pharetra fringilla nunc varius vitae.'
echo 'Condimentum justo finibus feugiat laoreet. Nullam finibus sem semper nunc'
echo 'hendrerit. Maecenas suscipit magna sit amet augue pharetra, a iaculis nunc.'

echo ''



if [ $# -ne 0 ]
  then
    echo '           '

    echo ''
